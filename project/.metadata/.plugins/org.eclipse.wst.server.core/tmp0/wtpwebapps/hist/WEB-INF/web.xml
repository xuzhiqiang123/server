<!-- <!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" > -->

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <servlet-name>TestServlet</servlet-name>
    <servlet-class>servlets.TestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TestServlet</servlet-name>
    <url-pattern>/servlets/TestServlet</url-pattern>
  </servlet-mapping>
  
  <listener>
    <description>serverSocket</description>
    <listener-class>server.ServerSocketListener</listener-class>
  </listener>
  <listener>
    <description>socketio</description>
    <listener-class>server.ClientSocketIOListener</listener-class>
  </listener>
  
</web-app>
